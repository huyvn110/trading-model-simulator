{
    "productName": "Trade Tracker",
    "version": "1.0.0",
    "identifier": "com.tradetracker.app",
    "build": {
        "beforeDevCommand": "npm run build",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../out"
    },
    "app": {
        "windows": [
            {
                "title": "Trade Tracker",
                "width": 1400,
                "height": 900,
                "minWidth": 1024,
                "minHeight": 700,
                "resizable": true,
                "fullscreen": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}